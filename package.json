{
  "name": "uptime",
  "version": "0.0.1",
  "description": "serves uptime",
  "main": "src/index.ts",
  "scripts": {
    "start": "forever --watch -m 5 -o log/uptime.test.log --append -c \"ts-node\" src/index.ts --colors",
    "start:production": "BUILD_TYPE=prod forever --watch -m 5 -o log/uptime.prod.log --append -c \"ts-node\" src/index.ts --colors",
    "start:production:tsnode": "BUILD_TYPE=prod forever --watch -m 5 -o log/uptime.prod.log --append -c \"ts-node\" src/index.ts --colors",
    "build": "tsc",
    "build:docker": "docker build -t uptime ."
  },
  "author": "Ahmad Kanawi",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.11",
    "chalk": "^2.4.1",
    "forever": "^1.0.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "rate-limiter-flexible": "^0.19.4",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.32",
    "tslint": "^5.11.0",
    "typescript": "^3.4.2"
  }
}
