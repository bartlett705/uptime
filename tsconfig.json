      "allowJs": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "build",
      "sourceMap": true,
      "target": "es6",
    },

    "exclude": [
        "node_modules",
        "build",
        "jest",
      ]
}